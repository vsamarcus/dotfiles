[core]
	editor = code
[user]
	name = Marcus Vin√≠cius
	email = marcusv.sa@hotmail.com
[alias]
		ci = "!f() { git add -A && git commit -m \"$*\"; }; f"

		# Less verbose status
		st = status -sb

		# Checkout
		co = checkout

		# Checkout master
		cm = checkout master

		# Checkout staging
		cs = checkout staging

		# Checkout development
		cd = checkout development

		# Push user master
		pom = !(git push -u origin master)

		# Push master
		pm = !(git push origin master)

		# Push staging
		ps = !(git push origin staging)

		# Push development
		pd = !(git push origin development)

    cb = checkout -b

    sf = show --name-only

		# Pretty log
    lg = log --pretty=format:'%Cred%h%Creset %C(bold)%cr%Creset %Cgreen<%an>%Creset %s' --max-count=30
    

		hist = !git log --oneline --graph --decorate --all

		incoming = !(git fetch --quiet && git log --pretty=format:'%C(yellow)%h %C(white)- %C(red)%an %C(white)- %C(cyan)%d%Creset %s %C(white)- %ar%Creset' ..@{u})
    outgoing = !(git fetch --quiet && git log --pretty=format:'%C(yellow)%h %C(white)- %C(red)%an %C(white)- %C(cyan)%d%Creset %s %C(white)- %ar%Creset' @{u}..)
    unstage = reset HEAD --
    undo = checkout --
    rollback = reset --soft HEAD~1
